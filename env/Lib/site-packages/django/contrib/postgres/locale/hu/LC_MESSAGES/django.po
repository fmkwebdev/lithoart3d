#: .\env\Lib\site-packages\django\contrib\postgres\apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL kiterjesztések"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\env\Lib\site-packages\django\contrib\postgres\forms\array.py:16
#: .\env\Lib\site-packages\django\contrib\postgres\forms\array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "A tömb %(nth)s-ik eleme érvénytelen:"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "A belső tömböknek egyforma hosszúaknak kell lenniük."

#: .\env\Lib\site-packages\django\contrib\postgres\fields\hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "String-string/null leképezés"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\hstore.py:18
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "A(z) \"%(key)s\" nem karakterlánc, vagy null."

#: .\env\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:32
msgid "A JSON object"
msgstr "Egy JSON objektum"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:34
msgid "Value must be valid JSON."
msgstr "Az érték érvényes JSON kell legyen."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\hstore.py:16
msgid "Could not load JSON data."
msgstr "JSON adat betöltése sikertelen."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "A bemenetnek JSON szótárnak kell lennie."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\jsonb.py:19
#, python-format
msgid "“%(value)s” value must be valid JSON."
msgstr "A(z) \"%(value)s\" értékének érvényes JSON formátumban kell lennie."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:37
msgid "Enter two valid values."
msgstr "Adjon meg két érvényes értéket."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:38
msgid "The start of the range must not exceed the end of the range."
msgstr "A tartomány eleje nem lehet nagyobb a tartomány végénél."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:86
msgid "Enter two whole numbers."
msgstr "Adjon meg két egész számot."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:92
msgid "Enter two numbers."
msgstr "Adjon meg két számot."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:109
msgid "Enter two valid date/times."
msgstr "Adjon meg két érvényes dátumot/időt."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:115
msgid "Enter two valid dates."
msgstr "Adjon meg két érvényes dátumot."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"A lista %(show_value)d elemet tartalmaz, legfeljebb %(limit_value)d lehetne."
msgstr[1] ""
"A lista %(show_value)d elemet tartalmaz, legfeljebb %(limit_value)d lehetne."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"A lista %(show_value)d elemet tartalmaz, legalább %(limit_value)d kellene."
msgstr[1] ""
"A lista %(show_value)d elemet tartalmaz, legalább %(limit_value)d kellene."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Néhány kulcs hiányzik: %(keys)s"

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Néhány ismeretlen kulcs érkezett: %(keys)s"

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Bizonyosodjon meg róla, hogy a tartomány egésze kevesebb mint "
"%(limit_value)s."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Bizonyosodjon meg róla, hogy a tartomány egésze nagyobb mint %(limit_value)s."
